{
    "name": "vscode-sequence-diagrams",
    "displayName": "vscode-sequence-diagrams",
    "description": "Generates ULM sequence diagrams from simple text",
    "version": "0.0.1",
    "publisher": "aleksandardev",
    "engines": {
        "vscode": "^1.15.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.sayHello",
        "onLanguage:sequencediagram"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [{
            "command": "extension.sayHello",
            "title": "Hello World"
        }],
        "languages": [
            {
                "id": "sequencediagram",
                "aliases": [
                    "Sequence Diagram"
                ],
                "extensions": [
                    ".seqdiag"
                ]
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.5.2",
        "vscode": "^1.1.5",
        "mocha": "^3.5.0",
        "@types/node": "^8.0.26",
        "@types/mocha": "^2.2.42"
    }
}